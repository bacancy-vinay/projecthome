<h1>Skill#index</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Skills</th>
      <th>level</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <% @skills.each do |skill| %>
  <tr>
    <td><%= skill.skill_list %></td>
    <td><%= skill.rating %></td>
    <td><%= link_to "show", resume_skill_path(:id => skill.id) %></td>
    <td><%= link_to "edit", edit_resume_skill_path(:id => skill.id) %></td>
    <td><%= link_to "delete", resume_skill_path(:id => skill.id), method: :delete, data: {confirm:"Are you sure?"}%>
    </td>
    <% end %>
  </tr>
</table>
<div class="btn btn-info">
  <%= link_to "create new ", new_resume_skill_path, :class => "btn" %>
</div>

